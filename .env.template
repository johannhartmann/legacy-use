ANTHROPIC_API_KEY=xxx
API_KEY=your_api_key_here
VITE_API_URL=http://localhost:8088
VITE_SENTRY_DSN_UI=your_sentry_dsn_here
SENTRY_DSN_API=your_sentry_dsn_here
# API provider can be "anthropic", "bedrock", or "vertex" (default: "anthropic")
API_PROVIDER=anthropic
# AWS credentials for Bedrock (required only when API_PROVIDER is "bedrock")
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-west-2

VERTEX_REGION=us-east1
VERTEX_PROJECT_ID=your_vertex_project_id

# Database Configuration
# Use either SQLite or PostgreSQL connection string

# SQLite example:
DATABASE_URL=sqlite:///server/server.db

# PostgreSQL example (commented out by default):
# DATABASE_URL=postgresql://postgres:password@localhost:5432/server

# Posthog
VITE_PUBLIC_POSTHOG_KEY=your_posthog_key
VITE_PUBLIC_POSTHOG_HOST=https://eu.i.posthog.com
VITE_PUBLIC_DISABLE_TRACKING=false

# Security flag
VITE_ALLOW_OPENVPN=false
SHOW_DOCS=true
HIDE_INTERNAL_API_ENDPOINTS_IN_DOC=true
